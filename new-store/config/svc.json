{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "#NAME",
    "labels": {
      "app": "#NAME"
    }
  },
  "spec": {
    "selector": {
      "app": "#NAME"
    },
    "ports": [
      {
        "port": 80,
        "protocol": "TCP",
        "name": "http"
      },
      {
        "port": 443,
        "protocol": "TCP",
        "name": "https"
      }
    ],
    "type": "LoadBalancer"
  }
}
